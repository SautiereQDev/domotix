[run]
source = domotix
omit =
    */tests/*
    */__pycache__/*
    */venv/*
    */.venv/*

[report]
show_missing = true
skip_covered = false
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:

[html]
directory = htmlcov
